{
    "name": "@mkdocs-ts/notebook",
    "description": "Notebook plugins for mkdocs-ts.",
    "version": "0.1.4-wip",
    "author": "reinisch.gui@gmail.com",
    "homepage": "https://github.com/mkdocs-ts/notebook#README.md",
    "main": "./dist/@mkdocs-ts/notebook.js",
    "types": "src/index.ts",
    "scripts": {
        "clean": "del-cli dist",
        "setup": "python .w3nest/setup.py",
        "build": "yarn build:prod",
        "pre-build": "yarn clean",
        "build:dev": "yarn pre-build && webpack --mode development",
        "build:prod": "yarn pre-build && webpack --mode production",
        "lint-check": "yarn lint-prettier-check && yarn lint-eslint-check",
        "lint-prettier-check": "prettier --check ./src",
        "lint-prettier-write": "prettier --write ./src",
        "lint-eslint-check": "eslint ./src",
        "test": "jest -i",
        "test-coverage": "jest -i --collect-coverage",
        "doc": "(cd .w3nest && npx tsx doc.ts && python doc.py)",
        "publish-npm-public": "yarn build:prod && yarn test-coverage && yarn publish --access public"
    },
    "exports": {
        ".": {
            "default": "./dist/@mkdocs-ts/notebook.js",
            "types": "./src/index.ts",
            "webpm": {
                "dependencies": [
                    "mkdocs-ts",
                    "rx-vdom",
                    "rxjs",
                    "rxjs/fetch",
                    "@w3nest/ui-tk/Trees",
                    "esprima"
                ],
                "aliases": []
            }
        },
        "./Doc": {
            "default": "./dist/Doc.js",
            "types": "./src/doc/index.ts",
            "webpm": {
                "dependencies": [
                    "mkdocs-ts",
                    "@w3nest/webpm-client"
                ],
                "aliases": []
            }
        }
    },
    "dependencies": {
        "@fortawesome/free-solid-svg-icons": "^6.7.2",
        "@mkdocs-ts/code-api": "^0.2.0",
        "@w3nest/ui-tk": "^0.1.7",
        "@w3nest/webpm-client": "^0.1.8",
        "esprima": "^4.0.1",
        "mkdocs-ts": "^0.5.2",
        "prism-code-editor": "^4.0.0",
        "rx-vdom": "^0.1.3",
        "rxjs": "^7.5.6"
    },
    "devDependencies": {
        "@types/jest": "^29.5.4",
        "@types/node": "^22.13.4",
        "@types/webpack": "^5.28.5",
        "del-cli": "^5.1.0",
        "eslint": "9.14.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-jest": "^28.9.0",
        "isomorphic-fetch": "^3.0.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "jest-junit": "^16.0.0",
        "prettier": "^3.2.5",
        "prettier-plugin-properties": "^0.3.0",
        "ts-jest": "^29.2.5",
        "ts-loader": "9.5.2",
        "ts-node": "10.9.2",
        "tsx": "^4.19.2",
        "typedoc": "^0.27.7",
        "typescript": "5.7.3",
        "typescript-eslint": "8.14.0",
        "webpack": "^5.98.0",
        "webpack-bundle-analyzer": "^4.10.2",
        "webpack-cli": "6.0.1"
    },
    "webpm": {
        "kind": "esm",
        "dependencies": {
            "@mkdocs-ts/code-api": "^0.2.0",
            "@w3nest/ui-tk": "^0.1.7",
            "@w3nest/webpm-client": "^0.1.8",
            "esprima": "^4.0.1",
            "mkdocs-ts": "^0.5.2",
            "rx-vdom": "^0.1.3",
            "rxjs": "^7.5.6"
        }
    },
    "webpack": {
        "root": "src",
        "apiVersion": "01",
        "main": "./index.ts",
        "additionalEntries": {
            "Doc": "./doc/index.ts"
        },
        "externals": {
            "mkdocs-ts": [
                "mkdocs-ts_APIv05"
            ],
            "rx-vdom": [
                "rx-vdom_APIv01"
            ],
            "rxjs": [
                "rxjs_APIv7"
            ],
            "rxjs/fetch": [
                "rxjs_APIv7/fetch"
            ],
            "@w3nest/ui-tk/Trees": [
                "@w3nest/ui-tk_APIv01/Trees"
            ],
            "esprima": [
                "esprima_APIv4"
            ],
            "@w3nest/webpm-client": [
                "@w3nest/webpm-client_APIv01"
            ]
        }
    }
}
