{
    "name": "@mkdocs-ts/code-api",
    "description": "Code API plugins for mkdocs-ts.",
    "version": "0.2.3-wip",
    "author": "reinisch.gui@gmail.com",
    "homepage": "https://github.com/mkdocs-ts/code-api#README.md",
    "main": "./dist/@mkdocs-ts/code-api.js",
    "files": [
        "dist",
        "assets",
        "src/mkapi-backends"
    ],
    "scripts": {
        "clean": "del-cli dist",
        "setup": "python .w3nest/setup.py",
        "build": "yarn build:prod",
        "pre-build": "yarn clean",
        "post-build": "del-cli dist/src/tests",
        "build:dev": "yarn pre-build && webpack --mode development && yarn post-build",
        "build:prod": "yarn pre-build && webpack --mode production && yarn post-build",
        "lint-check": "yarn lint-prettier-check && yarn lint-eslint-check",
        "lint-prettier-check": "prettier --check ./src",
        "lint-prettier-write": "prettier --write ./src",
        "lint-eslint-check": "eslint ./src",
        "test": "jest -i",
        "test-coverage": "jest -i --collect-coverage",
        "doc": "(cd .w3nest && npx tsx doc.ts && python doc.py)",
        "publish-npm-public": "yarn build:prod && yarn test-coverage && yarn publish --access public"
    },
    "exports": {
        ".": {
            "default": "./dist/@mkdocs-ts/code-api.js",
            "types": "./dist/src/index.d.ts",
            "webpm": {
                "dependencies": [
                    "mkdocs-ts",
                    "rx-vdom",
                    "rxjs",
                    "@w3nest/http-clients"
                ],
                "aliases": []
            }
        },
        "./Doc": {
            "default": "./dist/Doc.js",
            "types": "./dist/src/doc/index.d.ts",
            "webpm": {
                "dependencies": [
                    "mkdocs-ts",
                    "@w3nest/webpm-client"
                ],
                "aliases": []
            }
        }
    },
    "dependencies": {
        "@fortawesome/free-solid-svg-icons": "^6.7.2",
        "@w3nest/http-clients": "^0.1.5",
        "@w3nest/webpm-client": "^0.1.5",
        "mkdocs-ts": "^0.5.3",
        "rx-vdom": "^0.1.3",
        "rxjs": "^7.5.6"
    },
    "devDependencies": {
        "@types/jest": "^30.0.0",
        "@types/node": "^24.3.0",
        "@types/webpack": "^5.28.5",
        "del-cli": "^6.0.0",
        "eslint": "9.34.0",
        "eslint-config-prettier": "10.1.8",
        "eslint-plugin-jest": "^29.0.1",
        "isomorphic-fetch": "^3.0.0",
        "jest": "^30.1.2",
        "jest-environment-jsdom": "^30.1.2",
        "jest-junit": "^16.0.0",
        "prettier": "^3.6.2",
        "prettier-plugin-properties": "^0.3.0",
        "ts-jest": "^29.4.1",
        "ts-loader": "9.5.4",
        "ts-node": "10.9.2",
        "tsx": "^4.20.5",
        "typedoc": "^0.28.12",
        "typescript": "5.9.2",
        "typescript-eslint": "8.41.0",
        "webpack": "^5.101.3",
        "webpack-bundle-analyzer": "^4.10.2",
        "webpack-cli": "6.0.1"
    },
    "webpm": {
        "kind": "esm",
        "dependencies": {
            "@w3nest/http-clients": "^0.1.5",
            "@w3nest/webpm-client": "^0.1.5",
            "mkdocs-ts": "^0.5.3",
            "rx-vdom": "^0.1.3",
            "rxjs": "^7.5.6"
        }
    },
    "webpack": {
        "root": "src",
        "apiVersion": "02",
        "main": "./index.ts",
        "additionalEntries": {
            "Doc": "./doc/index.ts"
        },
        "externals": {
            "mkdocs-ts": [
                "mkdocs-ts_APIv05"
            ],
            "rx-vdom": [
                "rx-vdom_APIv01"
            ],
            "@w3nest/webpm-client": [
                "@w3nest/webpm-client_APIv01"
            ],
            "@w3nest/http-clients": [
                "@w3nest/http-clients_APIv01"
            ],
            "rxjs": [
                "rxjs_APIv7"
            ]
        }
    }
}
